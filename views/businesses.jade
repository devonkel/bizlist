extends layout

block content
  h1
    | Business Names on File
  ul.listings
    each business, i in businesses
      li 
        a(href='/businesses/show/#{business.value._id}') #{business.value.name}
        br
        | Location: #{business.value.city}, #{business.value.state} #{business.value.zip}
        br
        | Phone: #{business.value.phone}
        br
        | URL: #{business.value.website}
